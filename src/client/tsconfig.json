{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": ".",
    "lib": ["ES2022"]
  },
  "include": ["*.ts", "generated/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
